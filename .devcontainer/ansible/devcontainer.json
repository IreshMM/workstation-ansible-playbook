{
	"name": "Ansible Environment",
	"dockerComposeFile": ["../docker-compose.yaml"],
	"service": "ansible",
	"customizations": {
		"vscode": {
			"extensions": [
				"GitHub.copilot-chat",
				"GitHub.copilot",
				"ms-azuretools.vscode-docker",
				"ms-python.python",
				"redhat.ansible"
			]
		}
	},
	"shutdownAction": "none",
	"workspaceFolder": "/workspace",
	"remoteUser": "root"
}